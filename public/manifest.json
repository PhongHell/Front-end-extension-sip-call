{
  "manifest_version": 3,
  "name": "Sip call - WebRTC",
  "description": "Demo Sip call",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open tracker"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
